{
    "name": "react-redux-template",
    "description": "An example app using the MERN stack, with the main branch using React and Redux.",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": ">=10.14.2",
        "npm": ">=6.4.1",
        "yarn": ">=1.22.5"
    },
    "scripts": {
        "startDev": "cd ./server && npm run server && cd ../client && npm run start",
        "postinstall": "(cd server && npm install); (cd client && npm install)"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@upstatement/prettier-config": "^0.3.0"
    },
    "dependencies": {
        "node-notifier": "^8.0.1",
        "object-path": "^0.11.5",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "mongoose": "^5.9.1",
        "nodemon": "^2.0.2"
    }
}